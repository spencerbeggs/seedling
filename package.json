{
	"name": "seedling",
	"version": "0.0.0",
	"description": "This is a boilerplate repo I use when I want to spin up a new Node.JS project.",
	"main": "lib/index.js",
	"scripts": {
		"setup": "npm install npm-cache -g",
		"beautify": "./node_modules/gulp/bin/gulp.js beautify",
		"reinstall": "rm -rf node_modules;npm-cache install || npm install",
		"docs": "./node_modules/gulp/bin/gulp.js jsdoc",
		"build": "./node_modules/gulp/bin/gulp.js build",
		"aws": "./node_modules/gulp/bin/gulp.js aws",
		"dev": "NODE_ENV=development ./node_modules/gulp/bin/gulp.js dev",
		"test": "./node_modules/gulp/bin/gulp.js test",
		"postinstall": "./node_modules/gulp/bin/gulp.js json",
		"postuninstall": "./node_modules/gulp/bin/gulp.js json",
		"start": "node ./lib/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/spencerbeggs/seedling.git"
	},
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/spencerbeggs/seedling/issues"
	},
	"homepage": "https://github.com/spencerbeggs/seedling#readme",
	"dependencies": {
		"body-parser": "^1.12.2",
		"compression": "^1.4.3",
		"cookie-parser": "^1.3.3",
		"dynamic-reverse-proxy": "^0.6.0",
		"express": "^4.11.0",
		"express-device": "^0.3.11",
		"express-handlebars": "^2.0.1",
		"html-minifier": "^0.7.0",
		"lodash": "^3.9.0",
		"node-env-file": "^0.1.7",
		"normalize.css": "^3.0.2",
		"serve-favicon": "^2.2.0"
	},
	"browser": {
		"config": "./config/index.js"
	},
	"engines": {
		"node": "0.12.x",
		"npm": "2.x"
	},
	"browserify": {
		"transform": [
			[
				"reactify", {
					"es6": true
				}
			],
			"envify",
			"uglifyify"
		]
	},
	"devDependencies": {
		"aws-sdk": "^2.1.38",
		"backbone": "^1.1.2",
		"browser-sync": "^2.8.0",
		"browserify": "^11.0.0",
		"envify": "^3.4.0",
		"gulp": "git://github.com/gulpjs/gulp.git#4.0",
		"gulp-concat": "^2.4.1",
		"gulp-eol": "^0.1.1",
		"gulp-jsbeautifier": "1.0.1",
		"gulp-jsdoc": "^0.1.4",
		"gulp-jshint": "^1.8.6",
		"gulp-json-format": "^1.0.0",
		"gulp-less": "^3.0.2",
		"gulp-minify-css": "^1.0.0",
		"gulp-nodemon": "^2.0.2",
		"gulp-notify": "^2.0.0",
		"gulp-plumber": "^1.0.1",
		"gulp-prettydiff": "^1.0.0",
		"gulp-rename": "^1.2.0",
		"gulp-sourcemaps": "^1.5.1",
		"gulp-strip-debug": "^1.0.1",
		"gulp-uglify": "^1.0.1",
		"gulp-util": "^3.0.6",
		"gulp-watch": "^4.2.4",
		"hbsfy": "^2.2.0",
		"html": "0.0.10",
		"http-proxy": "^1.11.1",
		"jquery": "^2.1.3",
		"jshint-stylish": "^2.0.0",
		"less-plugin-autoprefix": "^1.4.0",
		"moment": "^2.10.3",
		"pretty-hrtime": "^1.0.0",
		"prettydiff": "^1.12.8",
		"react": "^0.13.1",
		"reactify": "^1.1.0",
		"recursive-readdir": "^1.2.1",
		"request": "^2.58.0",
		"uglifyify": "^3.0.1",
		"vinyl-buffer": "^1.0.0",
		"vinyl-source-stream": "^1.0.0",
		"watchify": "^3.2.1"
	}
}
